<view class="main"><view class="list_data"><view class="list"><view class="list_item"><view class="top"><view class="crash"><view><text>预约单号：</text><text>{{list_data.code}}</text></view><view>已核销</view></view></view><view class="middle"><view class="middle_left"><image src="{{list_data.cover_image}}" alt class="_img"></image></view><view class="middle_right"><view class="title_h1">{{''+list_data.name+''}}</view><view><text>场地：</text>{{list_data.room}}</view><view><text>场次：</text>{{list_data.seat}}</view><block wx:if="{{list_data.people_num!=0}}"><view><text>人数：</text>{{list_data.people_num+"人"}}</view></block></view></view></view></view></view><view class="information"><view class="list_box"><view class="information-title">预约信息</view><view class="information-time"><view>预约时段</view><view class="times listTime">{{''+list_data.make_year+" "+list_data.make_time+''}}</view></view><block wx:if="{{cancel_time!=0}}"><view class="information-bor"><view>核销时间</view><view class="times">{{''+list_data.cancel_time+''}}</view></view></block></view></view><block wx:if="{{list_data.make_user_info!=''}}"><view class="information"><view class="list_box"><view class="information-title">预约人</view><block wx:for="{{list_data.make_user_info}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="information-ren"><view class="information-one"><view>姓名：<label class="_span">{{item.name}}</label></view><view class="phone">手机号：<label style="margin-left:40rpx;" class="_span">{{item.mobile}}</label></view></view><view class="information-two">社区：<label class="_span">{{item.address}}</label></view></view></block></view></view></block><view class="information"><view class="list_box"><view class="information-title">预约费用</view><view class="information-price"><view class="weixin"><block wx:if="{{list_data.pay_type==2}}"><image src="https://img0.baidu.com/it/u=3873280508,2879221686&fm=253&fmt=auto&app=138&f=JPEG?w=566&h=500" alt class="_img"></image></block><block wx:else><u-icon vue-id="703d031b-1" name="red-packet-fill" bind:__l="__l"></u-icon></block><view>{{''+(list_data.pay_type==1?'余额支付':list_data.pay_type==2?'微信支付':'支付')+''}}</view></view><view class="num">￥<label class="_span">{{list_data.money}}</label></view></view></view></view></view>