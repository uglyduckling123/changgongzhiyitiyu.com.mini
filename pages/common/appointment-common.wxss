.main {
  background: #F8F8FA;
  height: 1600rpx;
}
.main .list_data {
  padding-top: 30rpx;
}
.main .list {
  padding: 0 40rpx;
  background: #F8F8FA;
  margin-top: 20rpx;
}
.main .list .list_item {
  background-color: #fff;
  padding: 20rpx 40rpx;
  border-radius: 20rpx;
}
.main .list .list_item .top {
  display: flex;
}
.main .list .list_item .top image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.main .list .list_item .top .crash {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.main .list .list_item .top .crash view:nth-child(1) text:nth-child(1) {
  font-weight: bold;
}
.main .list .list_item .top .crash view:nth-child(2) {
  color: #6E8f77;
}
.main .list .list_item .middle {
  width: 100%;
  display: flex;
  margin-top: 15rpx;
  border-top: 1px solid #dcdcdc;
  border-bottom: 1px solid #dcdcdc;
  padding: 30rpx 0;
}
.main .list .list_item .middle_left {
  width: 45%;
  height: 180rpx;
  margin-right: 5%;
}
.main .list .list_item .middle_left ._img {
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.main .list .list_item .middle_right {
  width: 50%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.main .list .list_item .middle_right .title_h1 {
  font-weight: 700;
}
.main .list .travel_time {
  margin-top: 10rpx;
}
.main .list .travel_time .icon_pop {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.main .list .travel_time .icon_pop .icon_img {
  display: flex;
  align-items: center;
}
.main .list .travel_time .icon_pop .icon_img ._img {
  width: 40rpx;
  margin-right: 10rpx;
  height: 40rpx;
  border-radius: 50%;
}
.main .information {
  padding: 0 40rpx;
  background: #F8F8FA;
  margin-top: 20rpx;
}
.main .information .list_box {
  background-color: #fff;
  padding: 20rpx 40rpx;
  border-radius: 20rpx;
}
.main .information .list_box .information-title {
  font-weight: 550;
  font-size: 30rpx;
}
.main .information .list_box .information-time {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 0;
  border-bottom: 1px solid #dcdcdc;
}
.main .information .list_box .information-time .times {
  color: #999;
}
.main .information .list_box .information-time .listTime {
  color: #000;
}
.main .information .list_box .information-bor {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 0;
}
.main .information .list_box .information-bor .times {
  color: #000;
}
.main .information .list_box .information-ren {
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  padding: 30rpx 0;
  border-bottom: 1px solid #dcdcdc;
}
.main .information .list_box .information-ren .information-one {
  justify-content: space-between;
  color: #000;
}
.main .information .list_box .information-ren .information-one ._span {
  margin-left: 70rpx;
}
.main .information .list_box .information-ren .information-one .phone {
  margin-top: 20rpx;
}
.main .information .list_box .information-ren .information-two {
  margin-top: 20rpx;
}
.main .information .list_box .information-ren .information-two ._span {
  margin-left: 70rpx;
}
.main .information .list_box .information-price {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 15rpx;
  padding: 20rpx 0;
}
.main .information .list_box .information-price .weixin {
  display: flex;
  align-items: center;
}
.main .information .list_box .information-price .weixin ._img {
  margin-right: 15rpx;
  width: 50rpx;
  height: 50rpx;
}
.main .information .list_box .information-price .num {
  color: #d96573;
}
.main .information .list_box .information-price .num ._span {
  font-size: 40rpx;
}
.main .tuikuan {
  display: flex;
  width: 100%;
  justify-content: flex-end;
  margin: 20rpx 0;
}
.main .tuikuan view:nth-child(1) {
  width: 25%;
  height: 65rpx;
  font-size: 27rpx;
  line-height: 65rpx;
  text-align: center;
  background-color: rgba(234, 232, 232, 0.47);
  border-radius: 15rpx;
}

